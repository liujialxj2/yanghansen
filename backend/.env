# ApostropheCMS 环境配置

# 数据库连接
APOS_MONGODB_URI=mongodb://127.0.0.1:27017/hansenyang

# 开发模式
NODE_ENV=development

# 服务器端口
PORT=3000

# 前后端通信密钥（必须与前端一致）
APOS_EXTERNAL_FRONT_KEY="hansenyangwebsite-dev-key"

# 默认语言
LANG=zh

# 会话密钥
APOS_SESSION_SECRET=hansenyangwebsite

# API密钥
APOS_API_KEY=dev_api_key_hansenyangwebsite
